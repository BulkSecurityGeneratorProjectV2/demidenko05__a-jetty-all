<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- All eclipse sources: -->
  <Match>
    <Package name="~org\.eclipse.*"/>
  </Match>
  <!-- All apache sources: -->
  <Match>
    <Package name="~org\.apache.*"/>
  </Match>
  <!-- All android generated sources: -->
  <Match>
    <Class name="~.*R\$.*"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.FactoryWebAppClassLoaderStd"/>
    <Method name="create"/>
    <Bug pattern="DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.BootStrapEmbeddedSwing"/>
    <Bug pattern="SE_BAD_FIELD_STORE"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.BootStrapEmbeddedHttpsSwing"/>
    <Bug pattern="SE_BAD_FIELD_STORE"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.BootStrapEmbeddedSwing"/>
    <Bug pattern="DM_EXIT"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.BootStrapEmbeddedHttpsSwing"/>
    <Bug pattern="DM_EXIT"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.BootStrapEmbeddedHttpsSwing$1"/>
    <Bug pattern="DM_EXIT"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.ajetty.FactoryWebAppClassLoaderEmbedded"/>
    <Method name="create"/>
    <Bug pattern="DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.android.ajetty.WebAppClassLoaderAndroid"/>
    <Method name="init"/>
    <Bug pattern="DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.web.servlet.WebTest"/>
    <Method name="doGet"/>
    <Bug pattern="XSS_REQUEST_PARAMETER_TO_SERVLET_WRITER"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.android.ajetty.AJetty"/>
    <Method name="onCreate"/>
    <Bug pattern="REC_CATCH_EXCEPTION"/>
  </Match>
  <!--EI_EXPOSE_REP/2 is resource unfriendly approach to avoid object's unexpected changes.
  In real program the most of objects (not only Date and arrays) is mutable.
  These are also POJO-unfriendly rules.
  Using annotation "immutable" is the best way to implements logic: "do not change
  mutable object outside or better - by annotated allowChangeImmutableFor[]".-->
  <Match>
     <Class name="org.beigesoft.ajetty.UserCredentials" />
     <Bug pattern="EI_EXPOSE_REP" />
  </Match>
  <Match>
     <Class name="org.beigesoft.ajetty.UserCredentials" />
     <Bug pattern="EI_EXPOSE_REP2" />
  </Match>
</FindBugsFilter>
